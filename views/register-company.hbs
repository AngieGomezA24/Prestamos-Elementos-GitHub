<div>
    <h1 class="display-4 titulo">Registro Empresa</h1>
    <div class="row">
        <div class="col-md-3">

        </div>
        <div class="col-md-6">
            <form action="/create-user" method="POST">
                <div class="form-group col-sm">
                    <label for="nit">Nit</label>
                    <input type="text" class="form-control" id="nit" name="nit" aria-describedby="emailHelp">
                </div>
                <div class="form-group col-sm">
                    <label for="nameEntity">Nombre de la entidad</label>
                    <input type="text" class="form-control" id="nameEntity" name="nameEntity">
                </div>

                <div class="form-group col-sm">
                    <label for="typeCompany">Tipo de empresa</label>
                    <input type="text" class="form-control" id="typeCompany" name="typeCompany">
                </div>



                <button type="submit" class="btn btn-primary bg-warning">Registrarse</button>
            </form>
        </div>
        <div class="col-md-3">

        </div>
    </div>
</div>