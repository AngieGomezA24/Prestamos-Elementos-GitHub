<div>
    <h1 class="display-4 titulo">Registro Servidores Publicos</h1>
    <div class="row">
        <div class="col-md-3">

        </div>
        <div class="col-md-6">
            <form action="/create-user" method="POST">
                <div class="form-group col-sm">
                    <label for="userPerson">Usuario</label>
                    <input type="email" class="form-control" id="userPerson" name="userPerson"
                        aria-describedby="emailHelp">
                </div>
                <div class="form-group col-sm">
                    <label for="password">Contraseña</label>
                    <input type="text" class="form-control" id="password" name="password">
                </div>

                <div class="form-group col-sm">
                    <label for="code">Código</label>
                    <input type="text" class="form-control" id="code">
                </div>

                <div class="form-group col-sm">
                    <label for="email">Correo</label>
                    <input type="email" class="form-control" id="email" name="email" placeholder="Ingrese el email"
                        required>
                </div>

                <div class="form-group col-sm">
                    <label for="phone">Telefono</label>
                    <input type="number" class="form-control" id="phone" name="phone">
                </div>


                <div class="form-group col-sm ">
                    <label for="beneficiary">Beneficiarios</label>
                    <input type="text" class="form-control" id="beneficiary" name="beneficiary">
                </div>

                <div class="form-group col-sm">
                    <label for="numberId">Número de identificación</label>
                    <input type="text" class="form-control" id="numberId" name="numberId">
                </div>

                <button type="submit" class="btn btn-primary bg-warning">Registrarse</button>
            </form>
        </div>
        <div class="col-md-3">

        </div>
    </div>
</div>