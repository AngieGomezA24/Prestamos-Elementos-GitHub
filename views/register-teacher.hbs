<div>
    <h1 class="display-4 titulo">Registro Docente</h1>
<div class="row">
    <div class="col-md-3">
        
    </div>
    <div class="col-md-6">
        <form action="/create-user" method="POST">
        <div class="form-group col-sm">
          <label for="usuario">Usuario</label>
          <input type="email" class="form-control" id="entradaUsuario" aria-describedby="emailHelp">
        </div>
        <div class="form-group col-sm">
          <label for="contrasena">Contraseña</label>
          <input type="password" class="form-control" id="entradaContrasena">
        </div>

        <div class="form-group col-sm">
          <label for="codigo">Código</label>
          <input type="password" class="form-control" id="entradaCodigo">
        </div>

        <div class="form-group col-sm">
          <label for="correo">Correo</label>
          <input type="password" class="form-control" id="entradaCorreo">
        </div>

        <div class="form-group col-sm">
          <label for="telefono">Telefono</label>
          <input type="password" class="form-control" id="entradaTelefono">
        </div>

        <button type="submit" class="btn btn-primary bg-warning">Registrarse</button>
      </form>
    </div>
    <div class="col-md-3">
        
    </div>
</div>
</div>